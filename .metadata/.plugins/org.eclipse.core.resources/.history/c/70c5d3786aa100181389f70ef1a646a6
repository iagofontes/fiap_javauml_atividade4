package Exercicio3;

public enum Cores {
	
	Branco("Branco", 21),
	Preto("Preto", 22);
	
	private Integer codigo;
	private String nome;
	
	public Cores(String nomeCor, Integer codCor) {
		this.codigo = codCor;
		this.nome = nomeCor;
	}
	
	public String recuperaNomeCor() {
		return this.nome;
	}
	
	public Integer recuperaCodigoCor() {
		return this.codigo;
	}
	
	@Override
	public String toString() {
		return "CÃ³digo: "+this.codigo+", Nome: "+this.nome;
	}
}
